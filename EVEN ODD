import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        if(oddeven(num)==1)
            System.out.println("even");
        else
            System.out.println("odd");
    }
    public static int oddeven(int n){
        if(n%2==0)
            return 1;
        else
            return 0;

    }
}
